local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player.PlayerGui

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 500, 0, 400)
mainFrame.Position = UDim2.new(0.5, -250, 0.5, -200)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
mainFrame.BackgroundTransparency = 0.4
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Visible = false
mainFrame.Parent = screenGui

local border = Instance.new("Frame")
border.Size = UDim2.new(1, 0, 1, 0)
border.BackgroundTransparency = 1
border.BorderSizePixel = 5
border.BorderColor3 = Color3.fromRGB(255, 165, 0)
border.Parent = mainFrame

local teleportButton = Instance.new("TextButton")
teleportButton.Size = UDim2.new(0, 300, 0, 60)
teleportButton.Position = UDim2.new(0.5, -150, 0.2, 0)
teleportButton.Text = "Teleport to Bridge"
teleportButton.Font = Enum.Font.GothamBold
teleportButton.TextSize = 28
teleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
teleportButton.BackgroundColor3 = Color3.fromRGB(0, 128, 255)
teleportButton.BorderSizePixel = 2
teleportButton.BorderColor3 = Color3.fromRGB(255, 165, 0)
teleportButton.Parent = mainFrame

local noclipButton = Instance.new("TextButton")
noclipButton.Size = UDim2.new(0, 300, 0, 60)
noclipButton.Position = UDim2.new(0.5, -150, 0.4, 0)
noclipButton.Text = "Toggle Noclip"
noclipButton.Font = Enum.Font.GothamBold
noclipButton.TextSize = 28
noclipButton.TextColor3 = Color3.fromRGB(255, 255, 255)
noclipButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
noclipButton.BorderSizePixel = 2
noclipButton.BorderColor3 = Color3.fromRGB(255, 165, 0)
noclipButton.Parent = mainFrame

local noclipStatus = Instance.new("TextLabel")
noclipStatus.Size = UDim2.new(0, 300, 0, 40)
noclipStatus.Position = UDim2.new(0.5, -150, 0.55, 0)
noclipStatus.Text = "Noclip: OFF"
noclipStatus.Font = Enum.Font.GothamBold
noclipStatus.TextSize = 24
noclipStatus.TextColor3 = Color3.fromRGB(255, 0, 0)
noclipStatus.BackgroundTransparency = 1
noclipStatus.Parent = mainFrame

local nameLabel = Instance.new("TextLabel")
nameLabel.Size = UDim2.new(0, 500, 0, 50)
nameLabel.Position = UDim2.new(0.5, -250, 0.85, 0)
nameLabel.Text = "by badass1ia and Player"
nameLabel.Font = Enum.Font.GothamBold
nameLabel.TextSize = 36
nameLabel.TextColor3 = Color3.fromRGB(255, 165, 0)
nameLabel.BackgroundTransparency = 1
nameLabel.TextStrokeTransparency = 0.5
nameLabel.Parent = mainFrame

local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 60, 0, 60)
closeButton.Position = UDim2.new(1, -70, 0, 10)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 30
closeButton.TextColor3 = Color3.fromRGB(255, 0, 0)
closeButton.BackgroundTransparency = 0.4
closeButton.Parent = mainFrame

local iconButton = Instance.new("ImageButton")
iconButton.Size = UDim2.new(0, 80, 0, 80)
iconButton.Position = UDim2.new(0, 10, 0.9, -90)
iconButton.Image = "rbxassetid://150794921" -- Example image ID, replace with your preferred icon
iconButton.Active = true
iconButton.Draggable = true
iconButton.Parent = screenGui

local function toggleGUI()
    mainFrame.Visible = not mainFrame.Visible
end

iconButton.MouseButton1Click:Connect(toggleGUI)

closeButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
end)

teleportButton.MouseButton1Click:Connect(function()
    local targetPosition = Vector3.new(-340, -3, -49043)
    player.Character:SetPrimaryPartCFrame(CFrame.new(targetPosition))
end)

-- Noclip functionality
local noclipEnabled = false
local function toggleNoclip()
    noclipEnabled = not noclipEnabled
    noclipStatus.Text = "Noclip: " .. (noclipEnabled and "ON" or "OFF")
    noclipStatus.TextColor3 = noclipEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
end

noclipButton.MouseButton1Click:Connect(toggleNoclip)

game:GetService("RunService").Stepped:Connect(function()
    if noclipEnabled and player.Character then
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
            end
        end
    end
end)
